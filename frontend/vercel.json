{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://3-141-187-2.sslip.io/api/:path*" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)\\.(js|css|png|jpg|svg|webp|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    },
    {
      "source": "/health",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
